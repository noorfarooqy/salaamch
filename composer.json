{
    "name": "noorfarooqy/salaamch",
    "description": "Salaam Clearing House",
    "type": "library",
    "require": {
        "laravel/framework": "^10.43",
        "noorfarooqy/noorauth": "^1.0",
        "darkaonline/l5-swagger": "^8.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Noorfarooqy\\Salaamch\\": "src/",
            "Noorfarooqy\\Salaamch\\Controllers\\": "app/Controllers/",
            "Noorfarooqy\\Salaamch\\Services\\": "app/Services/",
            "Noorfarooqy\\Salaamch\\Helpers\\": "app/Helpers/",
            "Noorfarooqy\\Salaamch\\Events\\": "app/Events/",
            "Noorfarooqy\\Salaamch\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "noorabdi",
            "email": "mnoor@salaammfbank.co.ke"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Noorfarooqy\\Salaamch\\SalaamchServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin-laravel": "^2.2",
        "orchestra/testbench": "^8.21",
        "guzzlehttp/guzzle": "^7.8",
        "pestphp/pest-plugin-type-coverage": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
